                @Html.DisplayFor(modelItem => modelItem.MinLight)
            </td>
            <td>
                @Html.DisplayFor(modelItem => modelItem.MaxPressure)
            </td>
            <td>
                @Html.DisplayFor(modelItem => modelItem.MinPressure)
            </td>
            <td>
                @Html.DisplayFor(modelItem => modelItem.MaxHumidity)
            </td>
            <td>
                @Html.DisplayFor(modelItem => modelItem.MinHumidity)
            </td>
