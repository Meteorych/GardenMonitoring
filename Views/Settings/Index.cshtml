            <th>
                @Html.DisplayNameFor(model => model.MaxHumidity)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.MinHumidity)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>

        <tr>
            <td>
                @Html.DisplayFor(modelItem => modelItem.MaxTemperature)
            </td>
            <td>
                @Html.DisplayFor(modelItem => modelItem.MinTemperature)
            </td>
            <td>
                @Html.DisplayFor(modelItem => modelItem.MaxLight)
